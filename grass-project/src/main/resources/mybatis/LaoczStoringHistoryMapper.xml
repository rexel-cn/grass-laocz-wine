<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rexel.laocz.mapper.LaoczStoringHistoryMapper">

    <resultMap type="LaoczStoringHistory" id="LaoczStoringHistoryResult">
        <result property="stroringHistorityId" column="stroring_histority_id"/>
        <result property="workOrderId" column="work_order_id"/>
        <result property="tenantId" column="tenant_id"/>
        <result property="liquorBatchId" column="liquor_batch_id"/>
        <result property="potteryAltarId" column="pottery_altar_id"/>
        <result property="state" column="state"/>
        <result property="potteryAltarApplyWeight" column="pottery_altar_apply_weight"/>
        <result property="weighingTank" column="weighing_tank"/>
        <result property="pumpId" column="pump_id"/>
        <result property="weighingTankWeight" column="weighing_tank_weight"/>
        <result property="weighingStartTime" column="weighing_start_time"/>
        <result property="weighingEndTime" column="weighing_end_time"/>
        <result property="storingTime" column="storing_time"/>
        <result property="areaName" column="area_name"/>
        <result property="fireZoneName" column="fire_zone_name"/>
        <result property="potteryAltarNumber" column="pottery_altar_number"/>
        <result property="createBy" column="create_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateBy" column="update_by"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <sql id="selectLaoczStoringHistoryVo">
        select stroring_histority_id,
               work_order_id,
               tenant_id,
               liquor_batch_id,
               pottery_altar_id,
               state,
               pottery_altar_apply_weight,
               weighing_tank,
               pump_id,
               weighing_tank_weight,
               weighing_start_time,
               weighing_end_time,
               storing_time,
               area_name,
               fire_zone_name,
               pottery_altar_number,
               create_by,
               create_time,
               update_by,
               update_time,
               remark
        from laocz_storing_history
    </sql>

    <select id="selectLaoczStoringHistoryList" parameterType="LaoczStoringHistory"
            resultMap="LaoczStoringHistoryResult">
        <include refid="selectLaoczStoringHistoryVo"/>
        <where>
            <if test="workOrderId != null  and workOrderId != ''">
                and work_order_id = #{workOrderId}
            </if>
            <if test="tenantId != null  and tenantId != ''">
                and tenant_id = #{tenantId}
            </if>
            <if test="liquorBatchId != null  and liquorBatchId != ''">
                and liquor_batch_id = #{liquorBatchId}
            </if>
            <if test="potteryAltarId != null ">
                and pottery_altar_id = #{potteryAltarId}
            </if>
            <if test="state != null ">
                and state = #{state}
            </if>
            <if test="potteryAltarApplyWeight != null ">
                and pottery_altar_apply_weight = #{potteryAltarApplyWeight}
            </if>
            <if test="weighingTank != null ">
                and weighing_tank = #{weighingTank}
            </if>
            <if test="pumpId != null ">
                and pump_id = #{pumpId}
            </if>
            <if test="weighingTankWeight != null ">
                and weighing_tank_weight = #{weighingTankWeight}
            </if>
            <if test="weighingStartTime != null ">
                and weighing_start_time = #{weighingStartTime}
            </if>
            <if test="weighingEndTime != null ">
                and weighing_end_time = #{weighingEndTime}
            </if>
            <if test="storingTime != null ">
                and storing_time = #{storingTime}
            </if>
            <if test="areaName != null  and areaName != ''">
                and area_name like concat('%', #{areaName}, '%')
            </if>
            <if test="fireZoneName != null  and fireZoneName != ''">
                and fire_zone_name like concat('%', #{fireZoneName}, '%')
            </if>
            <if test="potteryAltarNumber != null  and potteryAltarNumber != ''">
                and pottery_altar_number = #{potteryAltarNumber}
            </if>
        </where>
    </select>

    <insert id="batchLaoczStoringHistory" parameterType="java.util.List"
            useGeneratedKeys="true" keyProperty="stroringHistorityId">
        insert into laocz_storing_history
        ( stroring_histority_id , work_order_id , tenant_id , liquor_batch_id , pottery_altar_id , state ,
        pottery_altar_apply_weight , weighing_tank , pump_id , weighing_tank_weight , weighing_start_time ,
        weighing_end_time , storing_time , area_name , fire_zone_name , pottery_altar_number , create_by , create_time ,
        update_by , update_time , remark) values
        <foreach item="item" index="index" collection="list" separator=",">
            ( #{item.stroringHistorityId}, #{item.workOrderId}, #{item.tenantId}, #{item.liquorBatchId},
            #{item.potteryAltarId}, #{item.state}, #{item.potteryAltarApplyWeight}, #{item.weighingTank},
            #{item.pumpId}, #{item.weighingTankWeight}, #{item.weighingStartTime}, #{item.weighingEndTime},
            #{item.storingTime}, #{item.areaName}, #{item.fireZoneName}, #{item.potteryAltarNumber}, #{item.createBy},
            #{item.createTime}, #{item.updateBy}, #{item.updateTime}, #{item.remark})
        </foreach>
    </insert>

</mapper>
